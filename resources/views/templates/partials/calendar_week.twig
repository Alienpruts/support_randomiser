<div> Current date : {{ date }}</div>
<div> Current week number : {{ week.__toString() }}</div>

<a href="{{ path_for('week.getweek', {'year' : iterations.previous.year, 'weeknr' : iterations.previous.week}) }}">previous</a>
<a href="{{ path_for('week.getweek', {'year' : iterations.next.year, 'weeknr' : iterations.next.week}) }}">next</a>
<div class="row">
    {% for day in week %}
        {% if loop.index < 6 %}
            <div class="col-sm-2">
                <span>{{ day.__toString() }}</span>
                <span>{{ day.format('d/m/y') }}</span>
            </div>
        {% endif %}
    {% endfor %}

</div>